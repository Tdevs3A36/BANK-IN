{% extends './base_front.html.twig' %}

{% block title %}
	Afficher
{% endblock %}

{% block body %}
	<h1>Account{{ account.id }}</h1>

	<table class="table">
		<tbody>
        <tr>
			<th>Nom Complet</th>
			<td>{{ account.NomComplet }}</td>
        </tr>
        <tr>    
			<th>Numero Telephone</th>
			<td>{{ account.NumTel }}</td>
        </tr>
        <tr>    
			<th>Email</th>
			<td>{{ account.Email }}</td>
		</tr>
        <tr>	
            <th>Sexe</th>
			<td>{{ account.Sexe }}</td>
		</tr>
        <tr>	
            <th>Date de Naissance</th>
			<td>{{ account.DateNaiss ? account.DateNaiss|date('Y-m-d') : '' }}</td>
		</tr>
        <tr>	
            <th>Adresse</th>
			<td>{{ account.Adresse }}</td>
		</tr>
        <tr>	
            <th>Ville</th>
			<td>{{ account.Ville }}</td>
		</tr>
        <tr>	
            <th>Brochure</th>
			<td>{{ account.brochureFilename }}</td>
		</tr>	
        <tr>
            <th>Etat</th>
			<td>{{ account.etat}}</td>
		
	</tr>
</tbody>
</table>
<a href="{{ path('app_account_index') }}">Retour Ã  la liste des prets</a>
{% endblock %}
